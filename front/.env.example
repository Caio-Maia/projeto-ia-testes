# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:5000

# Build Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_I18N=true
REACT_APP_ENABLE_FEEDBACK=true

# Feedback Storage Mode
# Options:
#   - 'backend': Always save feedback to server (for teams/enterprises)
#   - 'local': Always save feedback locally in browser (for privacy)
#   - 'user-choice': Let user choose in the Dashboard (default)
REACT_APP_FEEDBACK_STORAGE=user-choice

# Analytics (Optional)
REACT_APP_ANALYTICS_ID=

# Gemini API (Optional - for direct frontend calls)
REACT_APP_GEMINI_API_KEY=your-gemini-api-key-for-frontend

# Chat GPT (Optional - for direct frontend calls)
REACT_APP_CHATGPT_API_KEY=your-chatgpt-api-key-for-frontend

# Security
REACT_APP_TOKEN_STORAGE=localStorage

# App Name
REACT_APP_NAME=AI Test Suite Platform
REACT_APP_DESCRIPTION=Platform for intelligent test generation and coverage analysis
